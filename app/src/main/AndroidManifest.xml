<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AnimationDemo"
        tools:targetApi="31">
        <activity android:name=".activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.HeartAnimActivity" />
        <activity android:name=".activity.HeartAnimActivity$ElasticBallActivity" />
        <activity android:name=".activity.TwoBallActivity" />
        <activity android:name=".activity.BallAnimActivity" />
        <activity android:name=".activity.WaveActivity" />
        <activity android:name=".activity.ViscosityAnimActivity" />
        <activity android:name=".activity.LoaddingAnimActivity" />
        <activity android:name=".activity.LikeAnimActivity" />
        <activity android:name=".activity.FailOrSuccessAnimActivity" />
        <activity android:name=".activity.SearchAnimActivity"></activity>
    </application>

</manifest>